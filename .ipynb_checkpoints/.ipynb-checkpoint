    "import sqlite3\n",
    "try:\n",
    "    conn = sqlite3.connect('ccacregdb')\n",
    "    cursor = conn.cursor()\n",
    "    print(\"Connect\")\n",
    "    regtable_sql='''\n",
    "                    CREATE TABLE IF NOT EXISTS\n",
    "                        appts(\n",
    "                            apptid INTEGER PRIMARY KEY,\n",
    "                            stud INTEGER,\n",
    "                            service TEXT,\n",
    "                            apptdate\n",
    "                        )\n",
    "                \n",
    "                '''\n",
    "    \n",
    "except sqlite3.Error as error:\n",
    "    print('Error')\n",
    "    print(error)\n",
    "#Always colse all of our resources\n",
    "    finally:\n",
    "    if cursor:\n",
    "        cursor.close()\n",
    "    if conn:\n",
    "        conn.close()\n",
    "\n",
